# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



GET     /CorpusPreparation_source                      controllers.HomeController.CorpusPreparation_source
GET     /CorpusPreparation_target                      controllers.HomeController.CorpusPreparation_target
GET     /CorpusPreparation_LM                          controllers.HomeController.CorpusPreparation_LM

POST    /CorpusPreparation_source_upload               controllers.HomeController.CorpusPreparation_source_upload
POST    /CorpusPreparation_target_upload               controllers.HomeController.CorpusPreparation_target_upload
POST    /CorpusPreparation_LM_upload                   controllers.HomeController.CorpusPreparation_LM_upload




# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
